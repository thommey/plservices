#ifndef LIST_H_
#define LIST_H_

struct listnode {
	void *data;
	struct listnode *next;
};

struct dlistnode {
	void *data;
	struct listnode *prev;
	struct listnode *next;
};

typedef struct listnode list_t;
typedef struct dlistnode dlist_t;

void memreport_list(void);
list_t *list_create(void);
void list_insert(list_t *list, void *data);
list_t *tree_get(list_t *tree);

#endif // LIST_H_
